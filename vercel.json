{
  "version": 2,
  "builds": [
    { "src": "frontend/package.json", "use": "@vercel/static-build" },
    { "src": "backend/api/graphql.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/graphql", "dest": "/backend/api/graphql.js" },
    { "src": "/(.*)", "dest": "/frontend/$1" }
  ]
}
